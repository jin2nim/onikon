{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport { ref, onMounted } from 'vue';\nimport cancelData from '../../assets/data/cancel.json'; // cancel.json 파일\n\n// 필터링된 오늘자 취소 요청 데이터\n\nexport default {\n  __name: 'CancelTable',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const todayCancelData = ref([]);\n    const headers = ref(['No.', '이름', '연락처', '이벤트명', '취소 사유', '승인여부']);\n\n    // 오늘 날짜 계산\n    const today = new Date().toISOString().split('T')[0]; // YYYY-MM-DD 형태로 오늘 날짜 추출\n\n    // JSON 데이터에서 오늘자 취소 요청만 필터링\n    onMounted(() => {\n      todayCancelData.value = cancelData.filter(item => item.cancelRequestDate === today);\n    });\n    const __returned__ = {\n      todayCancelData,\n      headers,\n      today,\n      ref,\n      onMounted,\n      get cancelData() {\n        return cancelData;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","cancelData","todayCancelData","headers","today","Date","toISOString","split","value","filter","item","cancelRequestDate"],"sources":["C:/Users/bs010/portfolioVan/vue-first/dashboard/src/components/compo/CancelTable.vue"],"sourcesContent":["<template>\r\n      <table>\r\n        <thead>\r\n          <tr>\r\n            <th v-for=\"(header, index) in headers\" :key=\"index\">{{ header }}</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"(row, index) in todayCancelData\" :key=\"index\">\r\n            <td>{{ index +1 }}</td>\r\n            <td>{{ row.name }}</td>\r\n            <td>{{ row.contact }}</td>\r\n            <td>{{ row.eventName }}</td>\r\n            <td>{{ row.cancelReason }}</td>\r\n            <td>{{ row.Status }}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n  </template>\r\n  \r\n  <script setup>\r\n  import { ref, onMounted } from 'vue';\r\n  import cancelData from '../../assets/data/cancel.json'; // cancel.json 파일\r\n  \r\n  // 필터링된 오늘자 취소 요청 데이터\r\n  const todayCancelData = ref([]);\r\n  const headers = ref(['No.','이름', '연락처', '이벤트명', '취소 사유', '승인여부']);\r\n  \r\n  // 오늘 날짜 계산\r\n  const today = new Date().toISOString().split('T')[0]; // YYYY-MM-DD 형태로 오늘 날짜 추출\r\n  \r\n  // JSON 데이터에서 오늘자 취소 요청만 필터링\r\n  onMounted(() => {\r\n    todayCancelData.value = cancelData.filter(item => item.cancelRequestDate === today);\r\n  });\r\n  </script>\r\n  \r\n  <style scoped>\r\n  table {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n    margin-top: 20px;\r\n  }\r\n  \r\n  th, td {\r\n    border: 1px solid #ddd;\r\n    padding: 8px;\r\n    text-align: center;\r\n  }\r\n  \r\n  th {\r\n    background-color: #f2f2f2;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  tr:nth-child(even) {\r\n    background-color: #f9f9f9;\r\n  }\r\n  \r\n  tr:hover {\r\n    background-color: #f1f1f1;\r\n  }\r\n  </style>\r\n  "],"mappings":";;AAqBE,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,UAAU,MAAM,+BAA+B,CAAC,CAAC;;AAExD;;;;;;;;IACA,MAAMC,eAAe,GAAGH,GAAG,CAAC,EAAE,CAAC;IAC/B,MAAMI,OAAO,GAAGJ,GAAG,CAAC,CAAC,KAAK,EAAC,IAAI,EAAE,KAAK,EAAE,MAAM,EAAE,OAAO,EAAE,MAAM,CAAC,CAAC;;IAEjE;IACA,MAAMK,KAAK,GAAG,IAAIC,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;;IAEtD;IACAP,SAAS,CAAC,MAAM;MACdE,eAAe,CAACM,KAAK,GAAGP,UAAU,CAACQ,MAAM,CAACC,IAAI,IAAIA,IAAI,CAACC,iBAAiB,KAAKP,KAAK,CAAC;IACrF,CAAC,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}